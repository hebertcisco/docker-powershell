{% assign psm1_files = site.data.files | where: "type", "psm1" %}

{% for file in psm1_files %}
    <tr>
        <td valign="top">{% include icons/powershell.liquid %}</td>
        <td>
            <a href="{{ file.dirPath }}"
            >{{ file.name }}</a
            >
        </td>
        <td align="right">{{ file.type }}</td>
    </tr>
{% endfor %}